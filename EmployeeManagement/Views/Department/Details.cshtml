@model Department

@{
    ViewData["Title"] = @Model.Name;
}

<h1>Employee Detail <a href="/employee/index">Go Back</a></h1>

<ul class="list-group list-group-flush">
    <li class="list-group-item">Name : @Model.Name</li>
    <li class="list-group-item">Code : @Model.Code</li>
    <li class="list-group-item">Start Date : @Model.StartDate</li>
    <li class="list-group-item">Manager's Name : @Model.ManagerName</li>

</ul>